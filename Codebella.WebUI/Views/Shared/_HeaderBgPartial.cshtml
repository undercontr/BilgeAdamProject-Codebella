@model HeaderPartialModel

<header class="masthead" style="background-image: url('@Model.BgUrl');max-height:200px">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-12">
                <div class="site-heading">
                    <h1>@Model.Title</h1>
                    <span class="subheading">@Model.SubHeader</span>
                </div>
            </div>
        </div>
    </div>
</header>